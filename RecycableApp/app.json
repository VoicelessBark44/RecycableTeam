{
  "expo": {
    "name": "RecycableApp",
    "slug": "RecycableApp",
    "scheme": "expo-router",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/images/Project_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/images/Project_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adriangonzalez.RecycableApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "41caeb22-3ce7-46bf-8bb8-a3b27d6c634c"
      }
    }
  }
}
